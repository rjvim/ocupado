{
    "name": "rjvim/ocupado",
    "description": "Availability package for Laravel",
    "keywords": [
        "Availability",
        "Booking",
        "Occupancy"
    ],
    "homepage": "https://github.com/rjvim/ocupado",
    "license": "MIT",
    "authors": [
        {
            "name": "Rajiv Seelam",
            "email": "rajiv@betalectic.com",
            "homepage": "https://betalectic.com",
            "role": "Developer"
        }
    ],
    "require": {

    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "orchestra/testbench": "~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Betalectic\\Ocupado\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Betalectic\\Ocupado\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Betalectic\\Ocupado\\OcupadoServiceProvider"
            ]
        }
    }
}
