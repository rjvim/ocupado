{
    "name": "rjvim/file-manager",
    "description": "Upload the files to your Laravel app",
    "keywords": [
        "FileManager"
    ],
    "homepage": "https://github.com/rjvim/file-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Rajiv Seelam",
            "email": "rajiv@betalectic.com",
            "homepage": "https://betalectic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "cloudinary/cloudinary_php": "^1.13",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "orchestra/testbench": "~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Betalectic\\FileManager\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Betalectic\\FileManager\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Betalectic\\FileManager\\FileManagerServiceProvider"
            ]
        }
    }
}
